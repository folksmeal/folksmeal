generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Lead {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  phone       String?
  company     String
  companySize String
  location    String
  message     String
  currentSetup String?
  createdAt   DateTime @default(now())
}
